{
    "name": "gamesmkt/fishpond-record",
    "description": "Addition fishpond library.",
    "license": "MIT",
    "authors": [
        {
            "name": "Fish",
            "email": "chao.yen.po@gmail.com"
        }
    ],
    "repositories": [
        {
            "type": "path",
            "url": "../fishpond"
        }
    ],
    "require": {
        "php": ">=7.2",
        "gamesmkt/fishpond": "dev-master",
        "illuminate/support": "~5.5 || ^6.8"
    },
    "require-dev": {
        "mockery/mockery": "^1.0",
        "phpunit/phpunit": "^8.0"
    },
    "autoload": {
        "psr-4": {
            "Gamesmkt\\FishpondRecord\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Gamesmkt\\FishpondRecord\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit",
        "test:coverage": "phpunit --coverage-text --coverage-clover=coverage.clover"
    },
    "config": {
        "sort-packages": true
    }
}
